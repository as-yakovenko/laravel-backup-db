{
    "name": "yakovenko/laravel-backup-db",
    "description": "Simple Laravel database backup command with automatic cleanup",
    "type": "library",
    "keywords": ["laravel", "backup", "database", "mysql", "command"],
    "license": "MIT",
    "authors": [
        {
            "name": "Yakovenko",
            "email": "paffen.web@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Yakovenko\\LaravelBackupDb\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yakovenko\\LaravelBackupDb\\LaravelBackupDbServiceProvider"
            ]
        }
    }
}
